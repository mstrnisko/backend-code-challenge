{
  "scripts": {
    "migrate:create": "npx mikro-orm migration:create",
    "migrate:up": "npx mikro-orm migration:up",
    "migrate:down": "npx mikro-orm migration:down",
    "migrate:fresh": "npx mikro-orm migration:fresh",
    "migrate:only-seed": "npx mikro-orm seeder:run",
    "migrate:seed": "npm run migrate:fresh && npm run migrate:only-seed",
    "build": "tsc",
    "build-watch": "tsc -w",
    "dev": "ts-node-dev --transpile-only src/index.ts",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint": "npm run typecheck && npm run eslint",
    "test": "npm run migrate:seed && NODE_ENV=test jest -c ./jest.config.ts -i --forceExit --detectOpenHandles"
  },
  "mikro-orm": {
    "useTsNode": true
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@mikro-orm/cli": "^6.0.5",
    "@mikro-orm/core": "^6.0.5",
    "@mikro-orm/migrations": "^6.0.5",
    "@mikro-orm/postgresql": "^6.0.5",
    "@mikro-orm/reflection": "^6.0.5",
    "@mikro-orm/seeder": "^6.0.5",
    "@sinclair/typebox": "^0.32.13",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.9",
    "@types/ramda": "^0.29.10",
    "dotenv": "^16.4.1",
    "fastify": "^4.25.2",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ramda": "^0.29.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@sudolabs-io/eslint-config-sudolabs": "^1.5.197",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^26.9.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0"
  }
}
